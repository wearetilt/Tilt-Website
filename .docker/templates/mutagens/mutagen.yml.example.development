sync:
  defaults:
    ignore:
      vcs: true
      paths:
        - .DS_Store # macOS files
    permissions:
      defaultFileMode: 644
      defaultDirectoryMode: 755
      defaultOwner: "id:82"
      defaultGroup: "id:82"
  DOCKER_PROJECT_NAME-mutagensync-volume:
    alpha: "./"
    beta: "docker://DOCKER_PROJECT_NAME_mutagen/var/www"
    mode: "two-way-resolved"
    ignore:
      paths:
        - "/dbdata/"
        - "/node_modules/"
        - "/vendor/"
