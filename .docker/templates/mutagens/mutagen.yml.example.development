sync:
  defaults:
    ignore:
      vcs: true
      paths:
        - .DS_Store # macOS files
    permissions:
      defaultFileMode: 644
      defaultDirectoryMode: 755
      defaultOwner: "id:82"
      defaultGroup: "id:82"
  tilt-website-mutagensync-volume:
    alpha: "./"
    beta: "docker://tilt-website_mutagen/var/www"
    mode: "two-way-resolved"
